_ACEOF

  cat >>confdefs.h <<_ACEOF
#define UPCRI_THREAD_BITS_OVERRIDE $UPCRI_THREAD_BITS_OVERRIDE
_ACEOF

  cat >>confdefs.h <<_ACEOF
#define UPCRI_ADDR_BITS_OVERRIDE $UPCRI_ADDR_BITS_OVERRIDE
_ACEOF

fi

# clang-upc[2c] needs to be told explicitly what PTS rep to use
